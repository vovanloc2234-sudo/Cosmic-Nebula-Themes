        "dark",
        "theme",
        "color-theme",
        "purple",
        "space",
        "nebula",
        "cosmic"
    ],
    "icon": "Visuals/icon.png",
    "galleryBanner": {
        "color": "#1a0b2e",
        "theme": "dark"
    },
    "activationEvents": ["*"],
    "contributes": {
        "themes": [
            {
                "label": "Cosmic Nebula",
                "uiTheme": "vs-dark",
