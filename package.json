            },
            {
                "label": "Cosmic Nebula Soft",
                "uiTheme": "vs-dark",
                "path": "./theme/cosmic-nebula-soft.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vovanloc2234-sudo/Cosmic-Nebula-Themes"
    },
    "bugs": {
        "url": "https://github.com/vovanloc2234-sudo/Cosmic-Nebula-Themes/issues"
    },
    "homepage": "https://github.com/vovanloc2234-sudo/Cosmic-Nebula-Themes#readme"
