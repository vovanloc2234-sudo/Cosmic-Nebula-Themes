{
    "name": "theme-cosmic-nebula",
    "version": "1.0.0",
    "displayName": "Cosmic Nebula Themes",
    "description": "A deep space theme with purple nebula colors and alien flora accents. Perfect for long coding sessions in the dark.",
    "publisher": "cosmic-themes",
    "license": "MIT",
    "main": "app.js",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": ["Themes"],
    "keywords": [
